void main()
{
    ACR_TriggerOnEnter();

    // Custom code goes here.
	
	if(ACR_RetrieveQuestState("bg_jaffee_oozyichor", GetEnteringObject()) == 1) {
	    object oMyWP1 = GetWaypointByTag("wp_ocrejel_jaffee"); 
	    object oMyWP2 = GetWaypointByTag("wp_greyooze6_jaffee");
	    object oMyCreature1 = CreateObject(OBJECT_TYPE_CREATURE, "010_cr_o_ocrejel", GetLocation(oMyWP1));
	    object oMyCreature2 = CreateObject(OBJECT_TYPE_CREATURE, "010_cr_o_greyooze6", GetLocation(oMyWP2));
    }

